{
	"manifest_version": 3,
	"name": "Chrome Tab Switcher",
	"description": "Switch to most recently used tab with a hotkey.",
	"version": "1.0",
	"permissions": [
		"tabs"
	],
	"background": {
		"service_worker": "background.js"
	},
	"commands": {
		"switch-to-last-used-tab": {
			"suggested_key": {
				"default": "Alt+Q"
			},
			"description": "Switch to last used tab"
		}
	}
}