{
	"manifest_version": 3,
	"name": "Chrome Tab Switcher",
	"description": "Switch to most recently used tab with a hotkey",
	"version": "1.4",
	"permissions": [
		"tabs"
	],
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_start"
		}
	]
}